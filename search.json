---
layout: null
---
[
{% assign items = "" %}
{% for page in site.pages %}
  {% if page.title and page.title != "Subcategories" and page.description and page.description != "" %}
    {% capture item %}
      {
        "title": {{ page.title | jsonify }},
        "url": {{ page.url | jsonify }},
        "description": {{ page.description | jsonify }}
      }
    {% endcapture %}
    {% assign items = items | append: item | append: "||" %}
  {% endif %}
{% endfor %}
{{ items | split: "||" | join: "," }}
]
