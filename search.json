---
layout: null
---
[
{% assign items = "" %}
{% for page in site.pages %}
  {% if page.title and page.title != "Subcategories" and page.description and page.description != "" %}
    {% capture item %}
      {
        "title": "{{ page.title | escape }}",
        "url": "{{ page.url | relative_url }}",
        "description": "{{ page.description | strip_html | strip_newlines | escape }}"
      }
    {% endcapture %}
    {% assign items = items | append: item | append: "||" %}
  {% endif %}
{% endfor %}
{{ items | split: "||" | join: "," }}
]
